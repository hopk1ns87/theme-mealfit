  {%- comment %}</locksmith:2b97>{% endcomment -%}
  {% for item in search.results %}
    {%- comment %}<locksmith:8a1f>{% endcomment -%}
      {%- include 'locksmith-variables', locksmith_scope: 'subject', locksmith_subject: item, locksmith_subject_parent: search %}{% if locksmith_transparent %}{% if locksmith_f769_forloop__index == nil %}{% assign locksmith_f769_forloop__index = 1 %}{% assign locksmith_f769_forloop__index0 = 0 %}{% else %}{% assign locksmith_f769_forloop__index = locksmith_f769_forloop__index | plus: 1 %}{% assign locksmith_f769_forloop__index0 = locksmith_f769_forloop__index0 | plus: 1 %}{% endif %}{% if locksmith_f769_forloop__index == 1 %}{% assign locksmith_f769_forloop__first = true %}{% else %}{% assign locksmith_f769_forloop__first = false %}{% endif %}{% if locksmith_f769_forloop__index == locksmith_f769_forloop__size %}{% assign locksmith_f769_forloop__last = true %}{% else %}{% assign locksmith_f769_forloop__last = false %}{% endif %}{% assign locksmith_f769_forloop__rindex = locksmith_f769_forloop__size | minus: locksmith_f769_forloop__index | minus: 1 %}{% assign locksmith_f769_forloop__rindex0 = locksmith_f769_forloop__size | minus: locksmith_f769_forloop__index0 | minus: 1 %}{% else %}{% continue %}{% endif -%}
